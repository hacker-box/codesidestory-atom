{
  "name": "codesidestory-atom",
  "main": "./dist/bundle",
  "version": "0.2.0",
  "description": "A Slack integration to Keep your code related conversation next to the code.",
  "keywords": [
    "slack",
    "atom",
    "code",
    "conversation"
  ],
  "activationCommands": {
    "atom-workspace": "codesidestory:init"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "3.0.0": "getProviders"
      }
    }
  },
  "repository": "https://github.com/hacker-box/codesidestory-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "etch": "^0.12.5",
    "firebase": "^4.1.3",
    "jwt-decode": "^2.2.0",
    "left-pad": "^1.1.3",
    "light-markdown": "^0.1.2",
    "lodash.get": "^4.4.2",
    "request": "^2.81.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1"
  }
